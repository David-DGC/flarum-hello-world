{
    name david-dgcflarum-hello-world,
    description Say hello to the world!,
    type flarum-extension,
    require {
        flarumcore ^1.0.0
    },
    autoload {
        psr-4 {david-dgcHelloWorld src}
    },
    extra {
        flarum-extension {
            title Hello World,
            icon {
                name fas fa-smile,
                backgroundColor #238c59,
                color #fff
            }
        }
    }
}